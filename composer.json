{
    "name": "non-death/api-helper",
    "description": "Wrap API controllers for Laravel framework based on spatie/laravel-fractal",
    "keywords": [
        "spatie",
        "laravel-fractal",
        "laravel",
        "fractal",
        "lumen",
        "api",
        "transform"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Marcelo G.",
            "email": "marceloaldo@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "spatie/laravel-fractal": "^5.7"
    },
    "autoload": {
        "psr-4": {
            "ND\\ApiHelper\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ND\\ApiHelper\\ApiHelperServiceProvider"
            ]
        }
    }
}
